end
function CorrectKeyNotifi()
	OrionLib:MakeNotification({
		Name = "Correct Key",
		Content = "Correct key you may now enter.",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end
function IncorrectKey()
	OrionLib:MakeNotification({
		Name = "Incorrect Key!",
		Content = "Please enter the correct key next time.",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

KeyTab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
		print(KeyInput)
	end	  
})
KeyTab:AddButton({
	Name = "Check Key",
	Callback = function()
      		if _G.KeyInput == _G.Key and _G.Key2 and _G.Key3
			 
MakeScriptHub()
CorrectKeyNotifi()
	else
IncorrectKey()
	end
  	end    
})

